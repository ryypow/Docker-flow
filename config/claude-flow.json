{
  "version": "2.0.0",
  "environment": "dockerflow",
  "mode": "production",
  "features": {
    "swarm": {
      "enabled": true,
      "topology": "mesh",
      "max_agents": 10,
      "default_agents": ["coder", "tester", "reviewer", "analyst"],
      "auto_spawn": true,
      "coordination": {
        "strategy": "adaptive",
        "load_balancing": true,
        "fault_tolerance": true
      }
    },
    "memory": {
      "enabled": true,
      "persist": true,
      "ttl": 86400,
      "max_size_mb": 512,
      "compression": true,
      "namespaces": {
        "session": true,
        "project": true,
        "user": false
      }
    },
    "neural": {
      "enabled": true,
      "wasm_simd": true,
      "training": {
        "auto_train": true,
        "pattern_learning": true,
        "performance_optimization": true
      },
      "models": [
        "coordination",
        "optimization",
        "prediction",
        "adaptation"
      ]
    },
    "github": {
      "enabled": true,
      "auto_pr": false,
      "code_review": true,
      "issue_tracking": true,
      "workflows": {
        "ci_cd": true,
        "release_automation": false
      }
    },
    "sparc": {
      "enabled": true,
      "modes": [
        "spec-pseudocode",
        "architect",
        "code",
        "tdd",
        "debug",
        "security-review",
        "docs-writer",
        "integration",
        "refactor"
      ],
      "default_mode": "tdd",
      "auto_progression": true
    }
  },
  "performance": {
    "concurrent_execution": true,
    "batch_operations": true,
    "caching": {
      "enabled": true,
      "strategy": "adaptive",
      "ttl": 3600
    },
    "resource_limits": {
      "max_memory_mb": 8192,
      "max_cpu_cores": 4,
      "max_concurrent_tasks": 20,
      "timeout_seconds": 300
    },
    "optimization": {
      "token_reduction": true,
      "parallel_processing": true,
      "smart_routing": true,
      "load_balancing": true
    }
  },
  "security": {
    "dangerous_commands": [
      "rm -rf",
      "dd if=",
      "mkfs",
      "format",
      "sudo rm",
      "> /dev/",
      "chmod -R 777",
      "chown -R"
    ],
    "allowed_directories": [
      "/workspace",
      "/tmp",
      "/app",
      "/home/dockerflow"
    ],
    "environment_isolation": true,
    "sandbox_mode": true
  },
  "logging": {
    "level": "info",
    "format": "structured",
    "destinations": ["console", "file"],
    "file_rotation": true,
    "max_files": 10,
    "max_size_mb": 100
  },
  "networking": {
    "ports": {
      "ui": 5000,
      "websocket": 5001,
      "api": 5002,
      "terminal": 5003,
      "ttyd": 7681
    },
    "cors": {
      "enabled": true,
      "origins": ["*"],
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
      "headers": ["*"]
    },
    "rate_limiting": {
      "enabled": false,
      "requests_per_minute": 100,
      "burst_limit": 20
    }
  },
  "integrations": {
    "vscode": {
      "enabled": false,
      "extensions": []
    },
    "jupyter": {
      "enabled": false,
      "kernels": ["python", "bash"]
    },
    "docker": {
      "enabled": true,
      "auto_cleanup": true,
      "resource_monitoring": true
    }
  },
  "hooks": {
    "pre_task": {
      "enabled": true,
      "commands": [
        "session-restore",
        "agent-assignment",
        "resource-check"
      ]
    },
    "post_task": {
      "enabled": true,
      "commands": [
        "memory-update",
        "pattern-training",
        "metrics-collection"
      ]
    },
    "pre_edit": {
      "enabled": true,
      "commands": [
        "backup-file",
        "syntax-check"
      ]
    },
    "post_edit": {
      "enabled": true,
      "commands": [
        "format-code",
        "update-memory"
      ]
    },
    "session_end": {
      "enabled": true,
      "commands": [
        "export-metrics",
        "save-session",
        "cleanup-resources"
      ]
    }
  },
  "defaults": {
    "workspace": "/workspace",
    "shell": "/bin/bash",
    "editor": "vim",
    "python_version": "3.11",
    "node_version": "20",
    "timeout": 300,
    "max_output_size": 100000
  }
}